                                <ul class="dropdown-menu dropdown-menu-left animated-2x animated fadeIn">
                                    <li>
                                        <a href="/get-involved">Join In</a>
                                    </li>
                                    <li>
                                        <a href="/events">Meet Ups</a>
                                    </li>
                                    <li>
                                        <a href="/get-involved/merchendise">Merchendise</a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="/blog">Blog</a>
                            </li>
                            <li>
                                <a href="http://forums.dotnetfoundation.org">Forums</a>
                            </li>
                        </ul>
                    </div><!-- navbar-collapse -->
                </div><!-- container -->
            </nav>




